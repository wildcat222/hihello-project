<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="spring.hi_hello_spring.group.query.mapper.PeerReviewListMapper">

    <select id="findAllPeersReviewList" resultType="PeerReviewListAllQueryDTO">
        SELECT
            pl.peer_review_list_seq,
            pl.peer_review_list_content,
            pl.peer_review_list_score
        FROM
            peer_review_list AS pl
    </select>
</mapper>